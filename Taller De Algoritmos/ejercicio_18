Algoritmo Inicio_inicales
	Leer nombre
	Leer apellido1
	Leer apellido2
	inicial_del_nombre<-SubCadena(nombre,1,1)
	inicial_del_apellido1<-SubCadena(apellido1,1,1)
	inicial_del_apellido2<-SubCadena(apellido2,1,1)
	Escribir Mayusculas(inicial_del_nombre) Mayusculas(inicial_del_apellido1) Mayusculas(inicial_del_apellido2)
FinAlgoritmo
	
	